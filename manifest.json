{
  "manifest_version": 2,
  "name": "GazeTracing",
  "description": "stitp项目chrome插件展示平台",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "https://ajax.googleapis.com/",
    "http://api.flickr.com/"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}